{
	"name": "chain-game-bot",
	"version": "2.1.0",
	"private": true,
	"homepage": "https://discord.gg/GcwZtqNCGx",
	"type": "module",
	"main": "./dist/index.js",
	"scripts": {
		"build": "rm -rf ./dist && tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!./dist/\" --ignore-unknown --cache",
		"lint": "eslint",
		"serve": "nodemon --env-file .env --enable-source-maps . --ext js,json",
		"start": "node --enable-source-maps .",
		"test": "node --run build && node --enable-source-maps --test"
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"didyoumean2": "7.0.4",
		"discord.js": "14.17.3",
		"match-sorter": "8.0.0",
		"mongoose": "8.9.5",
		"strife.js": "4.3.2"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "3.2.1",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/node": "<=22.10",
		"eslint": "9.18.0",
		"eslint-config-cobaltt7": "1.1.0",
		"nodemon": "3.1.9",
		"prettier": "3.4.2",
		"typescript": "5.7.3"
	},
	"optionalDependencies": {
		"@railway/cli": "3.20.2"
	},
	"engines": {
		"node": "^22.10.0",
		"npm": ">=9.0.0"
	},
	"overrides": {
		"@discordjs/builders": "1.10.0",
		"@discordjs/collection": "2.1.1",
		"@discordjs/formatters": "0.6.0",
		"@discordjs/rest": "2.4.2",
		"@discordjs/util": "1.1.2-dev.1736104405-15cbca941",
		"@discordjs/ws": "2.0.1",
		"discord-api-types": "0.37.119"
	}
}
